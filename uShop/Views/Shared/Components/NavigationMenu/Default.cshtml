@model IEnumerable<string><div class="catalog__side">    <ul class="leftMenu">        <li class="first"><a href="/">Главная</a></li>        @foreach (string category in Model)
        {
            string url = "/Catalog/Brand/" + category;            <li class="@(category == ViewBag.SelectedCategory? "active": "")"><a href="@url">@category</a></li>        }    </ul></div>